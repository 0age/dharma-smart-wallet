{
  "extends": "strict",
  "rules": {
    "indent": ["warn", 2],
    "not-rely-on-time": false,
    "no-inline-assembly": false,
    "avoid-low-level-calls": false,
    "avoid-call-value": false,
    "no-complex-fallback": false,
    "payable-fallback": false,
    "no-empty-blocks": false,
    "function-max-lines": 70
  }
}